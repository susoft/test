<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context=".MainActivity" >
    
    <ImageView        
        android:id="@+id/main01"        
        android:layout_width="wrap_content"        
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="10dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:contentDescription="@string/action_settings"
        android:src="@drawable/logo" />
    
	<LinearLayout
        android:id="@+id/linearlayout0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/main01"
        android:baselineAligned="false" >
        
	    <LinearLayout
	        android:id="@+id/linearlayout1"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="0.5"
	        android:orientation="vertical" >
	        
	        <ImageView        
		        android:id="@+id/icon1"
		        android:layout_width="60dp"        
		        android:layout_height="60dp"
		        android:layout_gravity="center_horizontal"
		        android:layout_marginBottom="10dp"
		        android:contentDescription="@string/action_settings"
		        android:src="@drawable/plants0100" />       
		        
		    <TextView        
		        android:id="@+id/title1"        
		        android:layout_width="wrap_content"        
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_horizontal"
		        android:text="연합회소개"
		        android:textSize="17sp"
		        android:textStyle="bold" />
		</LinearLayout>
	
	    <LinearLayout
	        android:id="@+id/linearlayout2"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="0.5"
	        android:orientation="vertical" >
	        
	        <ImageView        
		        android:id="@+id/icon2"
		        android:layout_width="60dp"        
		        android:layout_height="60dp"
		        android:layout_gravity="center_horizontal"
		        android:layout_marginBottom="10dp"
		        android:contentDescription="@string/action_settings"
		        android:src="@drawable/plants0200" />       
		        
		    <TextView        
		        android:id="@+id/title2"        
		        android:layout_width="wrap_content"        
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_horizontal"
		        android:text="연합회현황"
		        android:textSize="17sp"
		        android:textStyle="bold" />
		</LinearLayout>
	    
	</LinearLayout>
	
	<LinearLayout
        android:id="@+id/linearlayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearlayout0"
        android:layout_marginTop="10dp"
        android:baselineAligned="false" >
        
	    <LinearLayout
	        android:id="@+id/linearlayout4"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="0.5"
	        android:orientation="vertical" >
	        
	        <ImageView        
		        android:id="@+id/icon4"
		        android:layout_width="60dp"        
		        android:layout_height="60dp"
		        android:layout_gravity="center_horizontal"
		        android:layout_marginBottom="10dp"
		        android:contentDescription="@string/action_settings"
		        android:src="@drawable/plants0300" />       
		        
		    <TextView        
		        android:id="@+id/title4"        
		        android:layout_width="wrap_content"        
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_horizontal"
		        android:text="교류회현황"
		        android:textSize="17sp"
		        android:textStyle="bold" />
		</LinearLayout>
	
	    <LinearLayout
	        android:id="@+id/linearlayout5"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="0.5"
	        android:orientation="vertical" >
	        
	        <ImageView        
		        android:id="@+id/icon5"
		        android:layout_width="60dp"        
		        android:layout_height="60dp"
		        android:layout_gravity="center_horizontal"
		        android:layout_marginBottom="10dp"
		        android:contentDescription="@string/action_settings"
		        android:src="@drawable/plants0400" />       
		        
		    <TextView        
		        android:id="@+id/title5"        
		        android:layout_width="wrap_content"        
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_horizontal"
		        android:text="회원사검색"
		        android:textSize="17sp"
		        android:textStyle="bold" />
		</LinearLayout>
	</LinearLayout>
	
	<LinearLayout
        android:id="@+id/linearlayout8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearlayout3"
        android:layout_marginTop="10dp"
        android:baselineAligned="false" >
        
	    <LinearLayout
	        android:id="@+id/linearlayout9"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="0.5"
	        android:orientation="vertical" >
	        
	        <ImageView        
		        android:id="@+id/icon6"
		        android:layout_width="60dp"        
		        android:layout_height="60dp"
		        android:layout_gravity="center_horizontal"
		        android:layout_marginBottom="10dp"
		        android:contentDescription="@string/action_settings"
		        android:src="@drawable/plants0500_1" />       
		        
		    <TextView        
		        android:id="@+id/title6"        
		        android:layout_width="wrap_content"        
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_horizontal"
		        android:text="교류회등록"
		        android:textSize="17sp"
		        android:textStyle="bold" />
		</LinearLayout>
	
	    <LinearLayout
	        android:id="@+id/linearlayout10"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="0.5"
	        android:orientation="vertical" >
	        
	        <ImageView        
		        android:id="@+id/icon7"
		        android:layout_width="60dp"        
		        android:layout_height="60dp"
		        android:layout_gravity="center_horizontal"
		        android:layout_marginBottom="10dp"
		        android:contentDescription="@string/action_settings"
		        android:src="@drawable/plants0600_1" />       
		        
		    <TextView        
		        android:id="@+id/title7"        
		        android:layout_width="wrap_content"        
		        android:layout_height="wrap_content"
		        android:layout_gravity="center_horizontal"
		        android:text="회원사등록"
		        android:textSize="17sp"
		        android:textStyle="bold" />
		</LinearLayout>
	</LinearLayout>
	
	<LinearLayout
        android:id="@+id/linearlayout7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="5dp"
        android:baselineAligned="false"
        android:orientation="vertical" >
        
	    <TextView 
	        android:layout_width="wrap_content"        
	        android:layout_height="wrap_content"
	        android:layout_gravity="center_horizontal"
	        android:text="광주광역시 광산구 도천동 621-15호"
	        android:textSize="12sp" />
		<TextView 
	        android:layout_width="wrap_content"        
	        android:layout_height="wrap_content"
	        android:layout_gravity="center_horizontal"
	        android:text="중소기업종합지원센터 5층"
	        android:textSize="12sp" />
		<TextView 
	        android:layout_width="wrap_content"        
	        android:layout_height="wrap_content"
	        android:layout_gravity="center_horizontal"
	        android:text="TEL: 062-956-7120 | FAX: 062-956-7121"
	        android:textSize="12sp" />
		<TextView 
	        android:layout_width="wrap_content"        
	        android:layout_height="wrap_content"
	        android:layout_gravity="center_horizontal"
	        android:text="Copyright 2012 © 중소기업융합광주전남연합회."
	        android:textSize="12sp" />
		<TextView 
	        android:layout_width="wrap_content"        
	        android:layout_height="wrap_content"
	        android:layout_gravity="center_horizontal"
	        android:text="All Rights Reserved."
	        android:textSize="12sp" />
	</LinearLayout>
</RelativeLayout>
