	<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal"
        android:layout_below="@id/linearlayout0104" >
        <Button
	        android:id="@+id/ntpbm_0104_btn01"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="0.5"
	        android:layout_marginRight="3dp"
	        android:text="@string/ntpbm_0104_btn01"
	        android:onClick="saveInstall"
	        style="@style/btnStyleBeige" />
	
	    <Button
	        android:id="@+id/ntpbm_0103_btn02"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_weight="0.5"
	        android:layout_marginLeft="3dp"
	        android:text="@string/ntpbm_0104_btn02"
	        android:onClick="cancelInstall"
	        style="@style/btnStyleBeige" />
	    
	</LinearLayout>
